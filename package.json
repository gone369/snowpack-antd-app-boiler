    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@snowpack/app-scripts-react": "^2.0.0",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-postcss": "^1.1.0",
    "@snowpack/plugin-react-refresh": "^2.4.0",
